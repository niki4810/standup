{
  "name": "Standup",
  "description": "A simple note management application for your standup meetings.",
  "version": "0.0.2",
  "manifest_version": 2,
  "app": {
    "launch": {
      "local_path": "dist/index.html",
      "container": "tab"
    }
  },
  "icons": { 
    "16": "app-icons/standup-icon-16x16.png",
    "48": "app-icons/standup-icon-48x48.png",
    "128": "app-icons/standup-icon-128x128.png" 
  }
}